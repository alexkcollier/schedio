{
  "name": "schedio-design",
  "version": "3.4.3",
  "private": true,
  "scripts": {
    "ase-encode": " node ./scripts/ase-encode.js",
    "add-unit-tests": "node ./scripts/add-all-missing-tests.js",
    "serve": "yarn run serve:storybook",
    "build": "vue-cli-service build --target lib ./src/index.js && rm dist/demo.html",
    "build:release": "node ./scripts/build-release.js",
    "lint": "run-p lint:vue lint:style",
    "build:storybook": "build-storybook -s ./static -c config/storybook",
    "dev": "run-p serve:storybook theo:onchange devtools",
    "devtools": "vue-devtools",
    "dist:tokens": "run-s theo ase-encode",
    "lint:style": "stylelint ./src/assets/**/*.scss ./src/**/*.vue",
    "lint:vue": "vue-cli-service lint ./{src,config,scripts}/**/*.js",
    "scaffold": "node ./scripts/scaffold-component.js",
    "semantic-release": "semantic-release",
    "serve:storybook": "start-storybook -p 6006 -s ./static -c ./config/storybook",
    "test": "run-s test:unit",
    "test:generate": "vue-cli-service test:unit --json --outputFile=.jest-test-results.json || true && echo `git commit -am 'test: updates test results'`",
    "test:unit": "vue-cli-service test:unit",
    "theo": "theo ./src/assets/styles/tokens/tokens.yml --setup ./config/theo/theo.config.js --transform web --format ase.json,color-map.scss,scss,raw.json,json --dest ./src/assets/styles/tokens/dist",
    "theo:onchange": "onchange \"./src/assets/styles/tokens/*.yml\" -- run-s theo ase-encode"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "core-js": "^2.6.9",
    "feather-icons": "^4.22.0",
    "lodash.startcase": "^4.4.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/gitlab": "^3.1.2",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@spartanbio/eslint-config": "^1.0.4",
    "@spartanbio/eslint-config-vue": "^1.0.0",
    "@storybook/addon-a11y": "^5.1.4",
    "@storybook/addon-jest": "^5.1.4",
    "@storybook/addon-knobs": "^5.1.4",
    "@storybook/addon-links": "^5.1.4",
    "@storybook/addons": "^5.1.4",
    "@storybook/vue": "^5.1.4",
    "@vue/babel-plugin-transform-vue-jsx": "^1.0.0",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-unit-jest": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/devtools": "^5.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ase-utils": "^0.1.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "babel-preset-vue": "^2.0.2",
    "chalk": "^2.4.2",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "fibers": "^4.0.1",
    "fs-extra": "^8.0.1",
    "get-contrast": "^2.0.0",
    "husky": "^2.4.0",
    "jeffsum": "^1.0.0",
    "jest-axe": "^3.1.1",
    "lint-staged": "^8.2.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.groupby": "^4.6.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.orderby": "^4.6.0",
    "lodash.throttle": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.0.0",
    "pure-color": "^1.3.0",
    "sass": "^1.21.0",
    "sass-loader": "^7.0.1",
    "semantic-release": "^15.13.12",
    "shelljs": "^0.8.3",
    "showdown": "^1.9.0",
    "showdown-highlight": "^2.1.3",
    "style-resources-loader": "^1.2.1",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.8.0",
    "theo": "^8.1.2",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10"
  },
  "homepage": "https://spartanbio-design.netlify.com",
  "husky": {
    "hooks": {
      "pre-commit": "sh ./scripts/guard-master.sh && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "sh ./scripts/guard-master.sh"
    }
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "stylelint ./src/**/*.vue",
      "git add"
    ],
    "*.scss": [
      "stylelint ./src/assets/**/*.scss",
      "git add"
    ]
  },
  "main": "dist/schedio-design.common.js",
  "browser": "dist/schedio-design.umd.min.js",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "gitlab:spartanbio-ux/schedio"
  },
  "vetur": {
    "tags": "dist/vetur/tags.json",
    "attributes": "dist/vetur/attributes.json"
  }
}
