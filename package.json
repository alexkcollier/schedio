{
  "name": "schedio-design",
  "version": "2.0.1",
  "private": true,
  "main": "dist/schedio-design.common.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "gitlab:spartanbio-marketing/schedio"
  },
  "homepage": "https://spartanbio-design.netlify.com",
  "scripts": {
    "build": "vue-cli-service build --target lib ./src/index.js --dashboard",
    "build:storybook": "build-storybook -c config/storybook",
    "dev": "run-p serve:storybook theo:onchange",
    "lint": "run-p lint:vue lint:style",
    "lint:style": "stylelint ./src/assets/**/*.scss ./src/**/*.vue",
    "lint:vue": "vue-cli-service lint ./{src,config,scripts}/**/*.js",
    "prepare": "node ./scripts/build.js",
    "scaffold": "node scripts/scaffold-component.js",
    "serve": "npm run serve:storybook",
    "serve:storybook": "start-storybook -p 6006 -c config/storybook",
    "theo": "theo ./src/assets/styles/tokens/tokens.yml --setup ./config/theo/theo.config.js --transform web --format color-map.scss,scss,raw.json,json --dest ./src/assets/styles/tokens/dist",
    "theo:onchange": "onchange \"./src/assets/styles/tokens/*.yml\" -- npm run theo",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "lodash.startcase": "^4.4.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/gitlab": "^3.0.5",
    "@semantic-release/release-notes-generator": "^7.1.2",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-knobs": "^4.0.2",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addon-options": "^4.0.2",
    "@storybook/addons": "^4.0.2",
    "@storybook/vue": "^4.0.2",
    "@vue/cli-plugin-babel": "^3.1.0",
    "@vue/cli-plugin-eslint": "^3.1.3",
    "@vue/cli-service": "^3.1.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "babel-preset-vue": "^2.0.2",
    "chalk": "^2.4.1",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "fs-extra": "^7.0.0",
    "husky": "^1.1.2",
    "lint-staged": "^8.0.4",
    "lodash.groupby": "^4.6.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.orderby": "^4.6.0",
    "node-sass": "^4.9.4",
    "node-sass-glob-importer": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.0.2",
    "prettier": "^1.14.3",
    "sass-loader": "^7.0.1",
    "semantic-release": "^15.10.6",
    "shelljs": "^0.8.2",
    "style-resources-loader": "^1.2.1",
    "stylelint": "^9.7.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.2",
    "theo": "^8.0.0",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-template-compiler": "^2.5.17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh scripts/guard-master.sh && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "sh scripts/guard-master.sh"
    }
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "stylelint ./src/**/*.vue",
      "git add"
    ],
    "*.scss": [
      "stylelint ./src/assets/**/*.scss",
      "git add"
    ]
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false,
          "tarballDir": "dist/"
        }
      ],
      "@semantic-release/gitlab",
      "@semantic-release/git"
    ]
  }
}
