{
  "name": "schedio-design",
  "version": "1.6.1",
  "private": true,
  "main": "dist/schedio.common.js",
  "repository": {
    "type": "git",
    "url": "gitlab:spartanbio-marketing/schedio"
  },
  "homepage": "https://spartanbio-design.netlify.com",
  "scripts": {
    "build": "vue-cli-service build --target lib --name schedio ./src/index.js",
    "build:storybook": "vue-cli-service build:storybook -c config/storybook",
    "dev": "run-p serve:storybook theo:onchange",
    "lint": "npm run lint:vue && npm run lint:style",
    "lint:style": "stylelint ./src/assets/**/*.scss",
    "lint:vue": "vue-cli-service lint ./src/*,./config/**/*.js",
    "prepare": "node ./scripts/build.js",
    "scaffold": "node scripts/scaffold-component.js",
    "serve": "npm run serve:storybook",
    "serve:storybook": "vue-cli-service serve:storybook -p 6006 -c config/storybook",
    "theo": "theo ./src/assets/styles/tokens/tokens.yml --setup ./config/theo/theo.config.js --transform web --format color-map.scss,scss,raw.json,json --dest ./src/assets/styles/tokens/dist",
    "theo:onchange": "onchange \"./src/assets/styles/tokens/*.yml\" -- npm run theo",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "lodash.startcase": "^4.4.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/commit-analyzer": "^6.0.1",
    "@semantic-release/git": "^7.0.4",
    "@semantic-release/gitlab": "^3.0.4",
    "@semantic-release/release-notes-generator": "^7.0.2",
    "@storybook/addon-actions": "^4.0.0-alpha.20",
    "@storybook/addon-knobs": "^4.0.0-alpha.20",
    "@storybook/addon-links": "^4.0.0-alpha.20",
    "@storybook/addon-options": "^4.0.0-alpha.20",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-standard": "^3.0.5",
    "chalk": "^2.4.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "fs-extra": "^7.0.0",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "lodash.groupby": "^4.6.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.orderby": "^4.6.0",
    "node-sass": "^4.9.4",
    "node-sass-glob-importer": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.0.2",
    "prettier": "^1.14.3",
    "sass-loader": "^7.0.1",
    "semantic-release": "^15.10.2",
    "shelljs": "^0.8.2",
    "style-resources-loader": "^1.2.1",
    "stylelint": "^9.6.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.1",
    "theo": "^8.0.0-beta.2",
    "vue-cli-plugin-storybook": "^0.4.8",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-template-compiler": "^2.5.17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh scripts/guard-master.sh && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "sh scripts/guard-master.sh"
    }
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint && vue-cli-service lint:style",
      "git add"
    ],
    "*.scss": [
      "stylelint ./src/assets/**/*.scss",
      "git add"
    ]
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false,
          "tarballDir": "dist/"
        }
      ],
      "@semantic-release/gitlab",
      "@semantic-release/git"
    ]
  }
}
